{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Start server",
      "type": "shell",
      "command": "python -u -m http.server 8080",
      "options": {
        "cwd": "${workspaceFolder}/TBC Website Idea/bionicle-collective",
        "env": {}
      },
      "isBackground": true,
      "problemMatcher": {
        "pattern": {
          "regexp": "^.*Serving HTTP on.*$",
          "file": 1,
          "location": 2,
          "message": 3
        },
        "background": {
          "activeOnStart": true,
          "beginsPattern": ".",
          "endsPattern": "port 8080"
        }
      },
      "presentation": {
        "reveal": "always",
        "panel": "dedicated"
      }
    }
  ]
}
